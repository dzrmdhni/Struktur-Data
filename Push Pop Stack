#include <iostream> 
#include <stack> 
using namespace std; 
  
int main() 
{ 
    int elm=0;
    stack<int> mystack; 
    mystack.push(1); 
    mystack.push(2); 
    mystack.push(3); 
    mystack.push(4); 
  
    mystack.pop(); 
    mystack.pop(); 
  
    while (!mystack.empty()) 
    { 
        cout << "Elemen dalam stack adalah: " << mystack.top()<<endl; 
        mystack.pop(); 
        elm++;
    } 
    cout << "\nJumlah elemen dalam stack adalah: " <<elm;
} 
